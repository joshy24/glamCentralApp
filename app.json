    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.glamscentral.ios",
      "buildNumber": "1.0.0"
    },
    "android": {
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "package": "com.glamscentral.android",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
          "apiKey": "AIzaSyC0dv-PrbLMUCMXnWpHDFoIGyFTuCPE0NM"
        }
      }
    }
  }
}
