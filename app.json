{
  "expo": {
    "name": "GlamCentral",
    "slug": "GlamCentral",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF9391"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
